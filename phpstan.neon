parameters:
    level: 5
    phpVersion: 80100
    paths:
        - src
    bootstrapFiles:
        - bootstrap.php
    ignoreErrors:
        - '#Access to an undefined property Sprout\\Helpers\\(Twig|Php)?View::\$.+#'
    excludePaths:
        - src/sprout/views
        - src/sprout/module_template
        - src/sprout/Helpers/phpqrcode
        - src/sprout/Helpers/TwoFactor

